{
  "manifest_version": 1,
  "name": "Github网页中文",
  "short_name": "GithubCN",
  "description": "基于原项目GithubCN进行优化的一款github提供中文版界面的浏览器插件，提供新的github功能界面中文版界面。",
  "version": "1.4",
  "homepage_url": "https://github.com/landeyucc/GithubCN-Pro",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["js/content.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
